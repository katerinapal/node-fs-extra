{
  "name": "fs-extra",
  "version": "11.1.1",
  "description": "fs-extra contains methods that aren't included in the vanilla Node.js fs package. Such as recursive mkdir, copy, and remove.",
  "engines": {
    "node": ">=14.14"
  },
  "homepage": "https://github.com/jprichardson/node-fs-extra",
  "repository": {
    "type": "git",
    "url": "https://github.com/jprichardson/node-fs-extra"
  },
  "keywords": [
    "fs",
    "file",
    "file system",
    "copy",
    "directory",
    "extra",
    "mkdirp",
    "mkdir",
    "mkdirs",
    "recursive",
    "json",
    "read",
    "write",
    "extra",
    "delete",
    "remove",
    "touch",
    "create",
    "text",
    "output",
    "move",
    "promise"
  ],
  "author": "JP Richardson <jprichardson@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "graceful-fs": "^4.2.0",
    "jsonfile": "^6.0.1",
    "universalify": "^2.0.0",
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "klaw": "^2.1.1",
    "klaw-sync": "^3.0.2",
    "minimist": "^1.1.1",
    "mocha": "^10.1.0",
    "nyc": "^15.0.0",
    "proxyquire": "^2.0.1",
    "read-dir-files": "^0.1.1",
    "standard": "^17.0.0",
    "jest": "^28.1.3"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./esm": "./lib/esm.mjs"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/"
  ],
  "scripts": {
    "lint": "standard",
    "test-find": "find ./lib/**/__tests__ -name *.test.js | xargs mocha",
    "test": "npm run lint && npm run unit && npm run unit-esm",
    "unit": "nyc node test.js",
    "unit-esm": "node test.mjs",
    "gen-test-suite-stat": "jest --verbose ./generated-tests/copySync_stat_js.test.js --coverage=true",
    "gen-test-suite-getStats": "jest --verbose ./generated-tests/copySync_getStats_js.test.js --coverage=true",
    "gen-test-suite-mkdirsSync": "jest --verbose ./generated-tests/copySync_make_dir_js.test.js --coverage=true",
    "gen-test-suite-copy-stat": "jest --verbose ./generated-tests/copy_stat_js.test.js --coverage=true",

    "gen-test-suite-move-stat": "jest --verbose ./generated-tests/move_stat_js.test.js --coverage=true",

    "gen-test-suite-moveSync-stat": "jest --verbose ./generated-tests/moveSync_stat_js.test.js --coverage=true",
    "gen-test-suite-moveSync-make_dir": "jest --verbose ./generated-tests/moveSync_make_dir_js.test.js --coverage=true",
    "gen-test-suite-moveSync-isParentRoot-doRename-rename-moveAcrossDevice": "jest --verbose ./generated-tests/moveSync_isParentRoot_doRename_rename_moveAcrossDevice_js.test.js --coverage=true"
    
  },
  "sideEffects": false
}
